{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport EventBus from '../Refresh';\nexport default {\n  name: 'HeaderPage',\n  data() {\n    return {\n      username: localStorage.getItem('username') || '',\n      isLoggedIn: !!localStorage.getItem('username')\n    };\n  },\n  created() {\n    EventBus.on('username-updated', this.updateUsername);\n    EventBus.on('userLoggedIn', this.LoggedIn);\n    EventBus.on('userLoggedOut', this.LoggedOut);\n  },\n  beforeUnmount() {\n    EventBus.off('username-updated', this.updateUsername);\n    EventBus.off('userLoggedIn', this.LoggedIn);\n    EventBus.off('userLoggedOut', this.LoggedOut);\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('access_token');\n      localStorage.removeItem('username');\n      this.username = '';\n      this.isLoggedIn = false;\n      EventBus.emit('userLoggedOut');\n      this.$router.push('/login');\n    },\n    updateUsername(newUsername) {\n      this.username = newUsername;\n      localStorage.setItem('username', newUsername);\n    },\n    LoggedIn(username) {\n      this.username = username;\n      this.isLoggedIn = true;\n      localStorage.setItem('username', username);\n    },\n    LoggedOut() {\n      localStorage.removeItem('access_token');\n      localStorage.removeItem('username');\n      this.username = '';\n      this.isLoggedIn = false;\n      EventBus.emit('userLoggedOut');\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["EventBus","name","data","username","localStorage","getItem","isLoggedIn","created","on","updateUsername","LoggedIn","LoggedOut","beforeUnmount","off","methods","logout","removeItem","emit","$router","push","newUsername","setItem"],"sources":["D:\\JetBrains\\PROJECTPY\\cite_store\\frontend\\src\\components\\HeaderPage.vue"],"sourcesContent":["<template>\r\n  <header>\r\n    <nav>\r\n      <ul>\r\n        <li><router-link to=\"/\">Главная</router-link></li>\r\n        <li><router-link to=\"/catalog\">За продуктами</router-link></li>\r\n        <li><router-link to=\"/login\">Вход</router-link></li>\r\n        <li>\r\n          <router-link to=\"/profile\" class=\"profile-link\">{{ username }}</router-link>\r\n          <button @click=\"LoggedOut\" class=\"logout-button\">Выйти</button>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport EventBus from '../Refresh';\r\n\r\nexport default {\r\n  name: 'HeaderPage',\r\n  data() {\r\n    return {\r\n      username: localStorage.getItem('username') || '',\r\n      isLoggedIn: !!localStorage.getItem('username'),\r\n    };\r\n  },\r\n  created() {\r\n    EventBus.on('username-updated', this.updateUsername);\r\n    EventBus.on('userLoggedIn', this.LoggedIn);\r\n    EventBus.on('userLoggedOut', this.LoggedOut);\r\n  },\r\n  beforeUnmount() {\r\n    EventBus.off('username-updated', this.updateUsername);\r\n    EventBus.off('userLoggedIn', this.LoggedIn);\r\n    EventBus.off('userLoggedOut', this.LoggedOut);\r\n  },\r\n  methods: {\r\n    logout() {\r\n      localStorage.removeItem('access_token');\r\n      localStorage.removeItem('username');\r\n      this.username = '';\r\n      this.isLoggedIn = false;\r\n      EventBus.emit('userLoggedOut');\r\n      this.$router.push('/login');\r\n    },\r\n    updateUsername(newUsername) {\r\n      this.username = newUsername;\r\n      localStorage.setItem('username', newUsername);\r\n    },\r\n    LoggedIn(username) {\r\n      this.username = username;\r\n      this.isLoggedIn = true;\r\n      localStorage.setItem('username', username);\r\n    },\r\n    LoggedOut() {\r\n      localStorage.removeItem('access_token');\r\n      localStorage.removeItem('username');\r\n      this.username = '';\r\n      this.isLoggedIn = false;\r\n      EventBus.emit('userLoggedOut');\r\n      this.$router.push('/login');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nheader {\r\n  background-color: #3a3a3a;\r\n  padding: 15px 20px;\r\n  t