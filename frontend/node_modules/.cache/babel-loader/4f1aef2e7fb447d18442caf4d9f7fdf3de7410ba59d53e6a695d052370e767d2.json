{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'HeaderPage',\n  data() {\n    return {\n      username: '',\n      isLoggedIn: false\n    };\n  },\n  created() {\n    // Получаем имя пользователя из локального хранилища\n    this.username = localStorage.getItem('username'); // Получаем имя пользователя\n    this.isLoggedIn = !!this.username; // Проверяем, вошел ли пользователь\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('access_token');\n      localStorage.removeItem('username');\n      this.username = '';\n      this.isLoggedIn = false;\n      this.$router.push('/login');\n    },\n    async login(credentials) {\n      try {\n        const response = await this.$http.post('/login', credentials);\n        const {\n          token,\n          username\n        } = response.data;\n        localStorage.setItem('access_token', token);\n        localStorage.setItem('username', username);\n        this.isLoggedIn = true;\n        this.username = username;\n        this.$router.push('/');\n      } catch (error) {\n        console.error('Ошибка входа:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","username","isLoggedIn","created","localStorage","getItem","methods","logout","removeItem","$router","push","login","credentials","response","$http","post","token","setItem","error","console"],"sources":["D:\\JetBrains\\PROJECTPY\\cite_store\\frontend\\src\\components\\HeaderPage.vue"],"sourcesContent":["<template>\r\n  <header>\r\n    <nav>\r\n      <ul>\r\n        <li><router-link to=\"/\">Главная</router-link></li>\r\n        <li><router-link to=\"/catalog\">За продуктами</router-link></li>\r\n        <li><router-link to=\"/login\">Вход</router-link></li>\r\n        <li v-if=\"isLoggedIn\">\r\n          Привет, {{ username }}!\r\n          <button @click=\"logout\">Выйти</button>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HeaderPage',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      isLoggedIn: false,\r\n    };\r\n  },\r\n  created() {\r\n    // Получаем имя пользователя из локального хранилища\r\n    this.username = localStorage.getItem('username'); // Получаем имя пользователя\r\n    this.isLoggedIn = !!this.username; // Проверяем, вошел ли пользователь\r\n  },\r\n  methods: {\r\n    logout() {\r\n      localStorage.removeItem('access_token');\r\n      localStorage.removeItem('username');\r\n      this.username = '';\r\n      this.isLoggedIn = false;\r\n      this.$router.push('/login');\r\n    },\r\n    async login(credentials) {\r\n      try {\r\n        const response = await this.$http.post('/login', credentials);\r\n        const {token, username} = response.data;\r\n\r\n        localStorage.setItem('access_token', token);\r\n        localStorage.setItem('username', username);\r\n        this.isLoggedIn = true;\r\n        this.username = username;\r\n        this.$router.push('/');\r\n      } catch (error) {\r\n        console.error('Ошибка входа:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nheader {\r\n  background-color: #3a3a3a;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\nnav ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nnav ul li {\r\n  display: inline;\r\n  margin: 0 15px;\r\n}\r\n</style>"],"mappings":";AAiBA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACF,QAAO,GAAIG,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,EAAE;IAClD,IAAI,CAACH,UAAS,GAAI,CAAC,CAAC,IAAI,CAACD,QAAQ,EAAE;EACrC,CAAC;EACDK,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPH,YAAY,CAACI,UAAU,CAAC,cAAc,CAAC;MACvCJ,YAAY,CAACI,UAAU,CAAC,UAAU,CAAC;MACnC,IAAI,CAACP,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,UAAS,GAAI,KAAK;MACvB,IAAI,CAACO,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACD,MAAMC,KAAKA,CAACC,WAAW,EAAE;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM,