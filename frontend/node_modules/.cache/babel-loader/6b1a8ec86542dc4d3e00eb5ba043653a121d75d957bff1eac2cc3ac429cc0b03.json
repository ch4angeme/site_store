{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport EventBus from '../Refresh';\nexport default {\n  name: 'HeaderPage',\n  data() {\n    return {\n      username: localStorage.getItem('username') || '',\n      isLoggedIn: !!localStorage.getItem('username')\n    };\n  },\n  created() {\n    EventBus.on('username-updated', this.updateUsername);\n    EventBus.on('userLoggedIn', this.LoggedIn);\n    EventBus.on('userLoggedOut', this.LoggedOut);\n  },\n  beforeDestroy() {\n    // Удаляем подписки на события при уничтожении компонента\n    EventBus.off('username-updated', this.updateUsername);\n    EventBus.off('userLoggedIn', this.LoggedIn);\n    EventBus.off('userLoggedOut', this.LoggedOut);\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem('access_token');\n      localStorage.removeItem('username');\n      this.username = '';\n      this.isLoggedIn = false;\n      EventBus.emit('userLoggedOut');\n      this.$router.push('/login');\n    },\n    updateUsername(newUsername) {\n      this.username = newUsername;\n      localStorage.setItem('username', newUsername); // Обновляем имя пользователя в локальном хранилище\n    },\n    LoggedIn(username) {\n      this.username = username;\n      this.isLoggedIn = true;\n      localStorage.setItem('username', username); // Сохраняем имя пользователя в локальном хранилище\n    },\n    LoggedOut() {\n      this.username = '';\n      this.isLoggedIn = false;\n    }\n  }\n};","map":{"version":3,"names":["EventBus","name","data","username","localStorage","getItem","isLoggedIn","created","on","updateUsername","LoggedIn","LoggedOut","beforeDestroy","off","methods","logout","removeItem","emit","$router","push","newUsername","setItem"],"sources":["D:\\JetBrains\\PROJECTPY\\cite_store\\frontend\\src\\components\\HeaderPage.vue"],"sourcesContent":["<template>\r\n  <header>\r\n    <nav>\r\n      <ul>\r\n        <li><router-link to=\"/\">Главная</router-link></li>\r\n        <li><router-link to=\"/catalog\">За продуктами</router-link></li>\r\n        <li><router-link to=\"/login\">Вход</router-link></li>\r\n        <li><router-link to=\"/profile\">\r\n          {{ username }}</router-link>\r\n          <button @click=\"logout\">Выйти</button>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport EventBus from '../Refresh';\r\n\r\nexport default {\r\n  name: 'HeaderPage',\r\n  data() {\r\n    return {\r\n      username: localStorage.getItem('username') || '',\r\n      isLoggedIn: !!localStorage.getItem('username'),\r\n    };\r\n  },\r\n  created() {\r\n    EventBus.on('username-updated', this.updateUsername);\r\n    EventBus.on('userLoggedIn', this.LoggedIn);\r\n    EventBus.on('userLoggedOut', this.LoggedOut);\r\n  },\r\n  beforeDestroy() {\r\n    // Удаляем подписки на события при уничтожении компонента\r\n    EventBus.off('username-updated', this.updateUsername);\r\n    EventBus.off('userLoggedIn', this.LoggedIn);\r\n    EventBus.off('userLoggedOut', this.LoggedOut);\r\n  },\r\n  methods: {\r\n    logout() {\r\n      localStorage.removeItem('access_token');\r\n      localStorage.removeItem('username');\r\n      this.username = '';\r\n      this.isLoggedIn = false;\r\n      EventBus.emit('userLoggedOut');\r\n      this.$router.push('/login');\r\n    },\r\n    updateUsername(newUsername) {\r\n      this.username = newUsername;\r\n      localStorage.setItem('username', newUsername); // Обновляем имя пользователя в локальном хранилище\r\n    },\r\n    LoggedIn(username) {\r\n      this.username = username;\r\n      this.isLoggedIn = true;\r\n      localStorage.setItem('username', username); // Сохраняем имя пользователя в локальном хранилище\r\n    },\r\n    LoggedOut() {\r\n      this.username = '';\r\n      this.isLoggedIn = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nheader {\r\n  background-color: #3a3a3a;\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\nnav ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nnav ul li {\r\n  display: inline;\r\n  margin: 0 15px;\r\n}\r\n</style>"],"mappings":";AAiBA,OAAOA,QAAO,MAAO,YAAY;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEC,YAAY,CAACC,OAAO,CAAC,UAAU,KAAK,EAAE;MAChDC,UAAU,EAAE,CAAC,CAACF,YAAY,CAACC,OAAO,CAAC,UAAU;IAC/C,CAAC;EACH,CAAC;EACDE,OAAOA,CAAA,EAAG;IACRP,QAAQ,CAACQ,EAAE,CAAC,kBAAkB,EAAE,IAAI,CAACC,cAAc,CAAC;IACpDT,QAAQ,CAACQ,EAAE,CAAC,cAAc,EAAE,IAAI,CAACE,QAAQ,CAAC;IAC1CV,QAAQ,CAACQ,EAAE,CAAC,eAAe,EAAE,IAAI,CAACG,SAAS,CAAC;EAC9C,CAAC;EACDC,aAAaA,CAAA,EAAG;IACd;IACAZ,QAAQ,CAACa,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACJ,cAAc,CAAC;IACrDT,QAAQ,CAACa,GAAG,CAAC,cAAc,EAAE,IAAI,CAACH,QAAQ,CAAC;IAC3CV,QAAQ,CAACa,GAAG,CAAC,eAAe,EAAE,IAAI,CAACF,SAAS,CAAC;EAC/C,CAAC;EACDG,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPX,YAAY,CAACY,UAAU,CAAC,cAAc,CAAC;MACvCZ,YAAY,CAACY,UAAU,CAAC,UAAU,CAAC;MACnC,IAAI,CAACb,QAAO,GAAI,EAAE;MAClB,IAAI,CAACG,UAAS,GAAI,KAAK;MACvBN,QAAQ,CAACiB,IAAI,CAAC,eAAe,CAAC;MAC9B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACDV,cAAcA,CAACW,WAAW,EAAE;MAC1B,IAAI,CAACjB,QAAO,GAAIiB,WAAW;MAC3BhB,YAAY,CAACiB,OAAO,CAAC,UAAU,EAAED,WAAW,CAAC,EAAE;IACjD,CAAC;IACDV,QAAQA,CAACP,QAAQ,EAAE;MACjB,IAAI,CAACA,QAAO,GAAIA,QAAQ;MACxB,IAAI,CAACG,UAAS,GAAI,IAAI;MACtBF,YAAY,CAACiB,OAAO,CAAC,UAAU,EAAElB,QAAQ,CAAC,EAAE;IAC9C,CAAC;IACDQ,SAASA,CAAA,EAAG;MACV,IAAI,CAACR,QAAO,GAAI,EAAE;MAClB,IAAI,CAACG,UAAS,GAAI,KAAK;IACzB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}