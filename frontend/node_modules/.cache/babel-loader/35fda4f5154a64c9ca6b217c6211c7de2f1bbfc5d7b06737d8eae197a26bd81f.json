{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      formData: {\n        username: '',\n        password: ''\n      },\n      responseMessage: ''\n    };\n  },\n  methods: {\n    async submitForm() {\n      try {\n        const response = await axios.post('http://127.0.0.1:5000/api/users/user/login', this.formData);\n\n        // Проверяем сообщение от сервера\n        this.responseMessage = response.data.message;\n\n        // Если авторизация успешна, перенаправляем пользователя\n        if (response.status === 200) {\n          Cookies.set('username', response.data.username);\n          Cookies.set('user_id', response.data.user_id);\n          this.$router.push('/catalog'); // Перенаправление на главную страницу\n        }\n      } catch (error) {\n        console.error('Ошибка при отправке данных:', error);\n        // Обработка ошибки\n        if (error.response) {\n          this.responseMessage = error.response.data.message || 'Ошибка при авторизации';\n        } else {\n          this.responseMessage = 'Ошибка при отправке данных';\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Cookies","name","data","formData","username","password","responseMessage","methods","submitForm","response","post","message","status","set","user_id","$router","push","error","console"],"sources":["D:\\JetBrains\\PROJECTPY\\cite_store\\frontend\\src\\views\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Авторизация</h2>\r\n    <form @submit.prevent=\"submitForm\">\r\n      <div>\r\n        <label for=\"username\">Имя: </label>\r\n        <input type=\"text\" v-model=\"formData.username\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"password\">Пароль: </label>\r\n        <input type=\"password\" v-model=\"formData.password\" required />\r\n      </div>\r\n      <button type=\"submit\">Войти</button>\r\n    </form>\r\n    <div>\r\n      <p>Еще не зарегистрированы?</p>\r\n    </div>\r\n    <div>\r\n      <ul>\r\n        <li><router-link to=\"/registration\">Нажмите сюда</router-link></li>\r\n      </ul>\r\n    </div>\r\n    <div v-if=\"responseMessage\">\r\n      <p>{{ responseMessage }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Cookies from 'js-cookie';\r\n\r\nexport default {\r\n  name: 'LoginForm',\r\n  data() {\r\n    return {\r\n      formData: {\r\n        username: '',\r\n        password: '',\r\n      },\r\n      responseMessage: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      try {\r\n        const response = await axios.post('http://127.0.0.1:5000/api/users/user/login', this.formData);\r\n\r\n        // Проверяем сообщение от сервера\r\n        this.responseMessage = response.data.message;\r\n\r\n        // Если авторизация успешна, перенаправляем пользователя\r\n        if (response.status === 200) {\r\n          Cookies.set('username', response.data.username);\r\n          Cookies.set('user_id', response.data.user_id);\r\n          this.$router.push('/catalog'); // Перенаправление на главную страницу\r\n        }\r\n      } catch (error) {\r\n        console.error('Ошибка при отправке данных:', error);\r\n        // Обработка ошибки\r\n        if (error.response) {\r\n          this.responseMessage = error.response.data.message || 'Ошибка при авторизации';\r\n        } else {\r\n          this.responseMessage = 'Ошибка при отправке данных';\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n\r\ndiv {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nbutton {\r\n  background-color: #3a3a3a;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #757575;\r\n}\r\n</style>"],"mappings":";AA6BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE;MACZ,CAAC;MACDC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,4CAA4C,EAAE,IAAI,CAACP,QAAQ,CAAC;;QAE9F;QACA,IAAI,CAACG,eAAc,GAAIG,QAAQ,CAACP,IAAI,CAACS,OAAO;;QAE5C;QACA,IAAIF,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3BZ,OAAO,CAACa,GAAG,CAAC,UAAU,EAAEJ,QAAQ,CAACP,IAAI,CAACE,QAAQ,CAAC;UAC/CJ,OAAO,CAACa,GAAG,CAAC,SAAS,EAAEJ,QAAQ,CAACP,IAAI,CAACY,OAAO,CAAC;UAC7C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAU,CAAC,EAAE;QACjC;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;QACnD;QACA,IAAIA,KAAK,CAACR,QAAQ,EAAE;UAClB,IAAI,CAACH,eAAc,GAAIW,KAAK,CAACR,QAAQ,CAACP,IAAI,CAACS,OAAM,IAAK,wBAAwB;QAChF,OAAO;UACL,IAAI,CAACL,eAAc,GAAI,4BAA4B;QACrD;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}